            <Font size="17.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ComboBox fx:id="titles" layoutX="318.0" layoutY="78.0" onAction="#ViewAnnouncement" prefHeight="25.0" prefWidth="562.0">
