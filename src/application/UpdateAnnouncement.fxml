            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <Label layoutX="318.0" layoutY="45.0" text="Choose Title" textFill="#002e62">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="courses" layoutX="48.0" layoutY="78.0" onAction="#ComboBoxButton" prefWidth="150.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
   </children>
</AnchorPane>
