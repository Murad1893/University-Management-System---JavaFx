<AnchorPane prefHeight="433.0" prefWidth="1206.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UpdateAnnounceController">
   <children>
      <Label layoutX="48.0" layoutY="45.0" text="Enter Course" textFill="#002e62">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="112.0" text="Announcement" textFill="#002e62">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextArea fx:id="announcement" layoutX="47.0" layoutY="149.0" prefHeight="175.0" prefWidth="1100.0" promptText="Enter Announcement" style="-fx-border-color: #002e62; -fx-border-radius: 0;" />
      <Button fx:id="update" layoutX="938.0" layoutY="350.0" mnemonicParsing="false" onAction="#UpdateAnnouncement" prefHeight="46.0" prefWidth="96.0" styleClass="submit" stylesheets="@../application.css" text="Update" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
         </padding>
      </Button>
      <Button fx:id="delete" layoutX="1045.0" layoutY="350.0" mnemonicParsing="false" onAction="#DeleteAnnouncement" prefHeight="46.0" prefWidth="96.0" styleClass="submit" stylesheets="@../application.css" text="Delete" textFill="WHITE">
